{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TaskManager error",
  "type": "object",
  "properties": {
      "error": {
          "type": "string"
      },
    "timestamp": {
        "existingJavaType": "java.time.LocalDateTime",
        "type": "string",
        "format": "date-time",
        "customDateTimePattern" : "dd-MM-yyyy HH:mm:ss",
        "customTimeZone" : ""
    }
  },
  "additionalItems": true,
  "required": [
      "error",
      "timestamp"
  ]
}
